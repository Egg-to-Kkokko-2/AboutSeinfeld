{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "lib": [
      "dom",
      "esnext"
    ],
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowJs": true,
    "moduleResolution": "node",
    "jsxImportSource": "@emotion/react"
  },
  "baseUrl": "./src",
  "paths": {
      "components/*": ["./components/*"],  
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "public", ".cache"],
}
